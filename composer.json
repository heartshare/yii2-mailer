    "type": "yii2-extension",
    "keywords": ["yii2","extension", "mailer", "swiftmailer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pawel Kania",
            "email": "pawel.kania@itzen.pl",
            "homepage": "http://itzen.pl/",
            "role": "Itzen.pl founder"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "itzen/yii2-setting": "*"
    },
    "autoload": {
        "psr-4": {
            "itzen\\mialer\\": ""
        }
    }
}
